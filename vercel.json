{
  "redirects": [
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "www.toolhexa.vercel.app" }],
      "destination": "https://toolhexa.vercel.app/:path*",
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "toolhexa.vercel.app" }],
      "destination": "https://toolhexa.vercel.app/:path*",
      "permanent": true
    }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/" }
  ]
}
