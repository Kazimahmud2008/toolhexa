{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "redirects": [
    {
      "source": "/(.*)",
      "has": [{ "type": "host", "value": "www.toolhexa.vercel.app" }],
      "destination": "https://toolhexa.vercel.app/:path*",
      "statusCode": 301
    },
    {
      "source": "/(.*)",
      "has": [{ "type": "host", "value": "http://toolhexa.vercel.app" }],
      "destination": "https://toolhexa.vercel.app/:path*",
      "statusCode": 301
    },
    {
      "source": "/(.*)",
      "has": [{ "type": "host", "value": "http://www.toolhexa.vercel.app" }],
      "destination": "https://toolhexa.vercel.app/:path*",
      "statusCode": 301
    }
  ],
  "rewrites": [
    { "source": "/:path*", "destination": "/index.html" }
  ]
}
